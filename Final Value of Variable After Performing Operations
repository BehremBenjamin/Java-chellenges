class Solution {
   public static int finalValueAfterOperations(String[] operations) {
        int output = 0;
        for (String s : operations) {
            switch (s) {
                    case "++X", "X++" : output+=1; break;
                    case "--X", "X--" : output-=1; break;
            }
            
         }
        return output;
   }
}
